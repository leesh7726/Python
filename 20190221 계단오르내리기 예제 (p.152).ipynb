    "import random\n",
    "position = 0\n",
    "walk = [position]\n",
    "steps = int(input(\"걸음수 : \"))\n",
    "\n",
    "for i in range(steps):\n",
    "    step = 1 if random.randint(0,1)  else  -1\n",
    "    position += step\n",
    "    walk.append(position)\n",
    "    \n",
    "    \n",
    "import matplotlib.pyplot as plt\n",
    "plt.plot(walk)\n",
    "plt.show()"
   ]
